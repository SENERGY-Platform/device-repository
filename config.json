    "hub_connection_state_topic": "gateway_log",
    "permissions_v2_url": "",
    "mongo_url": "mongodb://localhost:27017",
    "mongo_table": "devicerepository",
    "mongo_rights_collection": "rights",
    "mongo_device_collection": "device",
    "mongo_device_type_collection": "devicetype",
    "mongo_device_group_collection": "devicegroup",
