{
    "server_port": "8080",
    "enable_swagger_ui": false,
    "log_level": "CALL",
    "done_topic": "device_repository_done",
    "device_topic": "devices",
    "device_type_topic": "device-types",
    "device_group_topic": "device-groups",
    "hub_topic": "hubs",
    "protocol_topic": "protocols",
    "concept_topic": "concepts",
    "characteristic_topic": "characteristics",
    "aspect_topic": "aspects",
    "function_topic": "functions",
    "device_class_topic": "device-classes",
    "location_topic": "locations",
